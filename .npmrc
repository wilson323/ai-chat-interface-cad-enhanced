# AI Chat Interface npm配置
# 确保依赖安装的一致性和兼容性

# 使用legacy peer deps解决依赖冲突
legacy-peer-deps=true

# 审计级别设置
audit-level=moderate

# 禁用资金信息显示
fund=false

# 进度显示
progress=true

# 缓存设置
cache-min=3600

# 注册表设置
registry=https://registry.npmjs.org/

# 安全设置
audit=true

# 包锁定
package-lock=true

# 精确版本安装
save-exact=false 